<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- stroke描边
        width:描边的粗细
        color：描出的颜色
        dashGap：虚线的间距，如果不写或为0则为实线
        dashWidth：虚线的长度
    -->
    <stroke
        android:width="1dp"
        android:color="@color/white" />
    <!-- solid填充
        color：中间填充的颜色-->
    <solid android:color="@color/appcolor" />
</shape>
